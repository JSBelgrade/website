#!/bin/bash
GH_TOKEN=6c31fa5d1bffa8480c8a9db5300f25ff0fbc4ad3
GH_REF=github.com/JSBelgrade/website.git
rm -rf dist || exit 0;
mkdir dist;
node ./bin/build.js
( cd dist
 git init
 git add .
 git commit -m "Deployed to Github Pages Test"
 git push --force --quiet "https://${GH_TOKEN}@${GH_REF}" master:gh-pages > /dev/null 2>&1
)